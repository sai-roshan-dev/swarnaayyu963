{
  "expo": {
    "name": "Aayu",
    "slug": "voice-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "swarnaayu",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/aayu.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sai.swarnaayu" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/image.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "balaji_naga",
    "extra": {
      "eas": {
        "projectId": "2db3e96f-5651-480e-b423-9644cc5c2cc0"
      }
    }
  }
}
